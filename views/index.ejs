<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <meta name="description" content="HStockHub - Premium social media accounts including Instagram, Facebook, Gmail and more. Aged, verified accounts for marketing and business use.">
  <meta name="keywords" content="HStockHub, social media accounts, Instagram accounts, Facebook accounts, Gmail accounts, aged accounts, verified accounts">
  <title><%= typeof title !== 'undefined' ? title : 'HStockHub - Premium Social Media Accounts' %></title>
  <link rel="stylesheet" href="/css/style.css">
  <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet">
  <link href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.0.0-beta3/css/all.min.css" rel="stylesheet">
  <script src="https://cdn.tailwindcss.com"></script>
</head>
<body>
  <!-- Navigation -->
  <nav class="navbar navbar-expand-lg navbar-dark fixed-top shadow-lg modern-nav">
    <div class="container-fluid px-4">
      <a class="navbar-brand fw-bold text-white d-flex align-items-center" href="/">
        <div class="brand-icon me-3">
          <i class="fas fa-store-alt text-warning"></i>
        </div>
        <span class="brand-text">HStockHub</span>
      </a>
      <button class="navbar-toggler border-0" type="button" data-bs-toggle="collapse" data-bs-target="#navbarNav">
        <span class="navbar-toggler-icon"></span>
      </button>
      <div class="collapse navbar-collapse" id="navbarNav">
        <ul class="navbar-nav me-auto">
          <li class="nav-item"><a class="nav-link nav-link-modern" href="#home">Home</a></li>
          <li class="nav-item"><a class="nav-link nav-link-modern" href="#products">Products</a></li>
          <li class="nav-item"><a class="nav-link nav-link-modern" href="#pricing">Pricing</a></li>
          <li class="nav-item"><a class="nav-link nav-link-modern" href="#features">Features</a></li>
          <li class="nav-item"><a class="nav-link nav-link-modern" href="#reviews">Reviews</a></li>
          <li class="nav-item"><a class="nav-link nav-link-modern" href="#faq">FAQ</a></li>
        </ul>
        <ul class="navbar-nav d-flex flex-row gap-2 gap-lg-0">
          <% if (typeof user !== 'undefined' && user) { %>
            <li class="nav-item dropdown">
              <a class="nav-link dropdown-toggle user-menu-btn" href="#" id="navbarDropdown" role="button" data-bs-toggle="dropdown">
                <i class="fas fa-user-circle me-2"></i>
                <%= user.name %>
                <% if (typeof user.isAdmin !== 'undefined' && user.isAdmin) { %>
                  <span class="badge bg-warning text-dark ms-2">Admin</span>
                <% } %>
              </a>
              <ul class="dropdown-menu dropdown-menu-end modern-dropdown">
                <% if (typeof user.isAdmin !== 'undefined' && user.isAdmin) { %>
                  <li><a class="dropdown-item" href="/admin"><i class="fas fa-tachometer-alt me-2"></i>Dashboard</a></li>
                <% } %>
                <li><a class="dropdown-item" href="/cart"><i class="fas fa-shopping-cart me-2"></i>Cart</a></li>
                <li><hr class="dropdown-divider"></li>
                <li><a href="/logout" class="dropdown-item"><i class="fas fa-sign-out-alt me-2"></i>Logout</a></li>
              </ul>
            </li>
          <% } else { %>
            <li class="nav-item">
              <a class="btn btn-outline-light btn-modern me-2" href="/login">
                <i class="fas fa-sign-in-alt me-2"></i>Login
              </a>
            </li>
            <li class="nav-item">
              <a class="btn btn-warning btn-modern fw-bold" href="/register">
                <i class="fas fa-user-plus me-2"></i>Sign Up
              </a>
            </li>
          <% } %>
        </ul>
      </div>
    </div>
  </nav>

  <!-- Hero Section -->
  <section id="home" class="hero-section position-relative overflow-hidden">
    <!-- Background with animated gradient -->
    <div class="hero-bg"></div>

    <!-- Floating particles -->
    <div class="hero-particles">
      <div class="particle particle-1"></div>
      <div class="particle particle-2"></div>
      <div class="particle particle-3"></div>
      <div class="particle particle-4"></div>
    </div>

    <div class="container position-relative">
      <div class="row align-items-center min-vh-100 py-5">
        <div class="col-lg-6 col-md-7">
          <div class="hero-content">
            <!-- Trust badge -->
            <div class="trust-badge mb-4">
              <i class="fas fa-shield-check me-2"></i>
              <span class="fw-bold">Trusted by 10,000+ Customers</span>
              <div class="badge-stars">
                <i class="fas fa-star text-warning"></i>
                <i class="fas fa-star text-warning"></i>
                <i class="fas fa-star text-warning"></i>
                <i class="fas fa-star text-warning"></i>
                <i class="fas fa-star text-warning"></i>
              </div>
            </div>

            <!-- Main heading -->
            <h1 class="hero-title display-3 fw-bold mb-4">
              Premium Social Media
              <span class="text-accent">Accounts</span>
              <br>
              <span class="hero-subtitle">That Drive Results</span>
            </h1>

            <!-- Description -->
            <p class="hero-description lead mb-4 text-muted">
              Get verified Instagram, Facebook, Gmail, and premium accounts across 9+ platforms.
              Aged, trusted accounts perfect for marketing automation, business growth, and social media management.
            </p>

            <!-- Social proof -->
            <div class="social-proof mb-4">
              <div class="d-flex align-items-center mb-3">
                <div class="platform-icons">
                  <i class="fab fa-instagram platform-icon instagram"></i>
                  <i class="fab fa-facebook platform-icon facebook"></i>
                  <i class="fas fa-envelope platform-icon gmail"></i>
                  <i class="fab fa-tiktok platform-icon tiktok"></i>
                  <span class="ms-2 fw-bold text-dark">+5 More Platforms</span>
                </div>
              </div>
              <div class="stats">
                <div class="stat-item">
                  <span class="stat-number">50K+</span>
                  <span class="stat-label">Accounts Delivered</span>
                </div>
                <div class="stat-item">
                  <span class="stat-number">99.9%</span>
                  <span class="stat-label">Success Rate</span>
                </div>
                <div class="stat-item">
                  <span class="stat-number">24/7</span>
                  <span class="stat-label">Support</span>
                </div>
              </div>
            </div>

            <!-- CTA Buttons -->
            <div class="hero-buttons mb-4">
              <a href="#products" class="btn btn-primary btn-lg px-5 py-3 me-3 mb-3 hero-btn-primary">
                <i class="fas fa-shopping-cart me-2"></i>
                Browse Accounts
              </a>
              <a href="#features" class="btn btn-outline-primary btn-lg px-5 py-3 mb-3 hero-btn-secondary">
                <i class="fas fa-star me-2"></i>
                View Features
              </a>
            </div>

            <!-- Guarantee -->
            <div class="guarantee d-flex align-items-center">
              <i class="fas fa-check-circle text-success fs-5 me-3"></i>
              <span class="text-muted">30-Day money-back guarantee • Instant delivery • Lifetime support</span>
            </div>
          </div>
        </div>

        <div class="col-lg-6 col-md-5">
          <div class="hero-visual">
            <!-- Interactive dashboard mockup -->
            <div class="dashboard-mockup">
              <div class="mockup-header">
                <div class="mockup-dots">
                  <span></span>
                  <span></span>
                  <span></span>
                </div>
                <div class="mockup-title">HStockHub Dashboard</div>
              </div>
              <div class="mockup-content">
